    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.286.0",
    "next": "^14.2.15",
    "next-cloudinary": "^6.5.2",
    "next-themes": "^0.2.1",
    "node-cron": "^3.0.3",
    "react": "^18",
